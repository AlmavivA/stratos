    <inline>{"member_second_derivative_memory_consumption":{"member_id":"{{member_id}}","cluster_id":"{{cluster_id}}","network_partition_id":"{{network_partition_id}}","value":"{{member_second_derivative_memory_consumption}}"}}</inline>
